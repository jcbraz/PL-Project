{
    "workspace": {
        "members": "['crates/*']",
        "resolver": "2",
        "package": {
            "license": "MIT OR Apache-2.0",
            "edition": "2021",
            "rust-version": "1.64.0",
            "include": "['build.rs', 'src/**/*', 'Cargo.toml', 'Cargo.lock', 'LICENSE*', 'README.md', 'benches/**/*', 'examples/**/*', 'tests/**/*']"
        }
    },
    "profile": {
        "panic": "abort",
        "lto": "True",
        "release": {
            "debug": "1"
        },
        "dev": {
            "opt-level": "1"
        }
    }
}